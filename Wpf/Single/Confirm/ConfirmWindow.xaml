<Window x:Class="Wpf.Single.Confirm.ConfirmWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Wpf.Single.Confirm"
        mc:Ignorable="d"
        Title="ConfirmWindow" Height="300" Width="300">
    <Grid>
        <Button Name="Ok" Click="Ok_OnClick"></Button>
        <Button Name="Cancel" Click="Cancel_OnClick"></Button>
    </Grid>
</Window>

<Window x:Class="WpfApp.Single.Confirm.ConfirmWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp.Single.Confirm"
        mc:Ignorable="d"
        Title="ConfirmWindow" Height="600" Width="800">
    <StackPanel>
        <Button Name="Ok" Content="Ok" Click="Ok_OnClick" Height="48" Margin="330,10,365.6,0"></Button>
        <Button x:Name="Cancel"  Content="Cancel" Click="Cancel_OnClick" Margin="330,6,365.6,0" Height="48"/>
    </StackPanel>
</Window>

